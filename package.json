{
  "name": "airlock",
  "version": "1.0.0",
  "description": "Spacetraders.io api client built with typescript, generated via openapi-generator-cli",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "openapi-generator-cli generate",
    "build": "npm run generate && tsup src/index.ts --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "spacetraders",
    "openapi",
    "typescript",
    "fetch"
  ],
  "namespace": "spacejunk",
  "author": "jonchurch",
  "license": "ISC",
  "repository": {
      "type": "git",
      "url": "https://github.com/jonchurch/airlock.git"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
